<div id="<%= dom_id project %>">
  <p><strong>Title:</strong> <%= project.title %></p>
  <p><strong>Description:</strong> <%= project.description %></p>
  <p><strong>GitHub URL:</strong> <%= link_to project.github_url, project.github_url, target: "_blank" if project.github_url.present? %></p>
  <p><strong>Live URL:</strong> <%= link_to project.live_url, project.live_url, target: "_blank" if project.live_url.present? %></p>
  <p><strong>Featured Image:</strong> <%= project.featured_image %></p>
  <p><strong>Tags:</strong> <%= project.tags %></p>
</div>
